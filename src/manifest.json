{
  "manifest_version": 3,
  "name": "Coplit To Git",
  "version": "1.0.3",
  "description": "Automatically commit coplit codes to github after submission",
  "icons": {
    "16": "images/codeStates.png",
    "48": "images/codeStates.png",
    "128": "images/codeStates.png"
  },
  "action": {
    "default_popup": "static/popup.html"
  },
  "content_scripts": [
    {
      "css": ["css/content.css"],
      "js": ["scripts/content.js"],
      "matches": ["https://urclass.codestates.com/*"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "permissions": ["storage"]
}
